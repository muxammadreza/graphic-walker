{
  "name": "graphic-walker-home",
  "version": "1.0.0",
  "description": "![](https://img.shields.io/github/license/kanaries/graphic-walker) ![](https://img.shields.io/npm/v/@kanaries/graphic-walker) ![](https://img.shields.io/github/workflow/status/kanaries/graphic-walker/auto-build.yml)",
  "private": true,
  "scripts": {
    "dev": "bun run --filter @kanaries/graphic-walker dev",
    "build": "bun run --filter @kanaries/graphic-walker build",
    "build:watch": "bun run --filter @kanaries/graphic-walker build:dev:watch",
    "build:db": "bun run --filter @kanaries/duckdb-computation build",
    "test": "bun run --filter @kanaries/graphic-walker test",
    "typecheck": "bun run --filter @kanaries/graphic-walker typecheck && bun run --filter @kanaries/duckdb-computation typecheck"
  },
  "workspaces": [
    "packages/graphic-walker",
    "packages/duckdb-wasm-computation"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kanaries/graphic-walker.git"
  },
  "keywords": [
    "visualization",
    "EDA",
    "data-analysis"
  ],
  "author": "Kanaries Data Inc.",
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/Kanaries/graphic-walker/issues"
  },
  "resolutions": {
    "@types/react": "^19.x",
    "@types/react-dom": "^19.x"
  },
  "homepage": "https://github.com/Kanaries/graphic-walker#readme",
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.3.9"
  },
  "packageManager": "bun@1.3.9",
  "trustedDependencies": [
    "esbuild",
    "postinstall-postinstall"
  ]
}
