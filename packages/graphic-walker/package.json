{
  "name": "@kanaries/graphic-walker",
  "version": "0.5.0",
  "scripts": {
    "transit": "bun -e \"\"",
    "dev:front_end": "vite --host",
    "dev": "bun run build:dev:nowatch",
    "dev:watch": "bun run build:dev:watch",
    "dev:app": "bun run dev:front_end",
    "gen-schema": "bun scripts/create-json-schema.js",
    "build": "vite build && tspc --build && bun run gen-schema",
    "build:dev:nowatch": "vite build --mode development",
    "build:dev:watch": "vite build --mode development --watch --emptyOutDir false",
    "build:app": "vite build --config vite-app.config.ts",
    "serve": "vite preview",
    "type": "bunx tsc --project tsconfig.json --emitDeclarationOnly",
    "typecheck": "bunx tsc --noEmit",
    "test": "bun test src"
  },
  "files": [
    "dist",
    "src"
  ],
  "license": "Apache-2.0",
  "author": {
    "name": "Kanaries Data Inc.",
    "email": "support@kanaries.net"
  },
  "sideEffects": [
    "./dist/graphic-walker.es.js",
    "./dist/graphic-walker.umd.js"
  ],
  "main": "./dist/graphic-walker.umd.js",
  "module": "./dist/graphic-walker.es.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/graphic-walker.es.js",
      "require": "./dist/graphic-walker.umd.js"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "prettier": {
    "tabWidth": 4,
    "printWidth": 160,
    "singleQuote": true
  },
  "types": "./dist/index.d.ts",
