{
  "name": "playground",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:gw": "cd ../graphic-walker && bun run build",
    "build:db": "cd ../duckdb-wasm-computation && bun run build",
    "build": "bun run build:gw && bun run build:db && tsc && cp ../graphic-walker/public/* ./public && vite build",
    "typecheck": "bunx tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "prettier": {
    "tabWidth": 4,
    "printWidth": 160,
    "singleQuote": true
  },
  "dependencies": {
    "@duckdb/duckdb-wasm": "^1.32.0",
    "@headlessui/react": "1.7.12",
    "@heroicons/react": "^2.2.0",
    "@kanaries/duckdb-computation": "0.0.1",
    "@kanaries/graphic-walker": "0.5.0",
    "@vercel/analytics": "^1.6.1",
    "color-space": "2.0.1",
    "react": "^19.2.4",
    "react-color": "^2.19.3",
    "react-dom": "^19.2.4",
    "react-router": "^6.30.3",
    "react-router-dom": "^6.30.3",
    "react-syntax-highlighter": "^16.1.0",
    "styled-components": "^6.3.9"
  },
  "devDependencies": {
    "@types/react": "^19.2.14",
    "@types/react-color": "^3.0.13",
    "@types/react-dom": "^19.2.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.24",
    "eslint": "^8.57.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.9.3",
    "vite": "^5.4.21"
  },
  "packageManager": "bun@1.3.9"
}